{
  "compilerOptions": {
    "target": "es6",
    "module": "CommonJS",
    "sourceMap": false,
    "removeComments": true,
    "noEmitOnError": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "noImplicitAny": true
  }
}
